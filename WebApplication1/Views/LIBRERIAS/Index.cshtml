@model IEnumerable<WebApplication1.Models.LIBRERIAS>

@{
    ViewBag.Title = "Tabla Librerias";
}

<body style="background-color:#E8BB39;">
</body>
<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.LIBRO2.NOMBRE)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.USUARIO2.CONTRASEÑA)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.LIBRO2.NOMBRE)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.USUARIO2.CONTRASEÑA)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.ID }) |
                @Html.ActionLink("Details", "Details", new { id = item.ID }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.ID })
            </td>
        </tr>
    }

</table>
